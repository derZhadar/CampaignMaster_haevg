<UserControl x:Class="CampaignMaster.Controls.ctlInkToolbar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CampaignMaster"
             xmlns:misc="clr-namespace:CampaignMaster.Misc"
             xmlns:models="clr-namespace:CampaignMaster.ViewModels"
             mc:Ignorable="d"
             Loaded="UserControl_Loaded"
             SizeChanged="UserControl_SizeChanged"
             MaxHeight="810" d:DesignWidth="800">
    <UserControl.DataContext>
        <models:vmToolbar/>
    </UserControl.DataContext>
    <UserControl.Resources>
        <Style x:Key="ToolbarButton" TargetType="{x:Type Button}" BasedOn="{StaticResource RoundButton}">
            <Setter Property="Margin" Value="0,3"/>
            <Setter Property="Width" Value="70"/>
            <Setter Property="Height" Value="70"/>
            <Setter Property="BorderThickness" Value="3"/>
            <Setter Property="BorderBrush" Value="Gray"/>
            <Setter Property="Background" Value="Gainsboro"/>
        </Style>
        <Style x:Key="ColorButton" TargetType="{x:Type Button}" BasedOn="{StaticResource ToolbarButton}">
            <Setter Property="Margin" Value="5,5,0,0"/>
            <EventSetter Event="Click" Handler="ColorButton_Clicked"/>
            <Setter Property="Height" Value="70"/>
            <Setter Property="BorderThickness" Value="3"/>
            <Setter Property="BorderBrush" Value="Gray"/>
            <Setter Property="Background" Value="Gainsboro"/>
            <Setter Property="Tag" Value="True"/>
        </Style>
    </UserControl.Resources>
    <UserControl.RenderTransform>
        <TranslateTransform/>
    </UserControl.RenderTransform>
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="170"/>
            <ColumnDefinition Width="85"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>

            <Border Grid.Row="0" VerticalAlignment="Top" Background="{StaticResource Toolbar.Background}" CornerRadius="40,0,0,0" Margin="0">
                <StackPanel Margin="5" Orientation="Horizontal">
                    <Button Style="{StaticResource ToolbarButton}" Margin="5,3" Tag="-1" Click="SizeButton_Clicked">
                        <Border Margin="0,3,0,0" Width="25" Height="4" Background="Black"/>
                    </Button>
                    <Button Style="{StaticResource ToolbarButton}" Margin="5,3" Tag="1" Click="SizeButton_Clicked">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0*"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Border Margin="0,14,0,10" Width="25" Height="4" Background="Black" Grid.RowSpan="2"/>
                            <Border Margin="10,3,11,0" Width="4" Height="25" Background="Black" Grid.RowSpan="2"/>
                        </Grid>
                    </Button>
                </StackPanel>
            </Border>


            <Border Grid.Row="1" VerticalAlignment="Center" Background="{StaticResource Toolbar.Background}" CornerRadius="0,0,0,0" Margin="0,10,0,0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <StackPanel x:Name="pnlToolBar" Grid.Column="0" Margin="5,5,0,5" Orientation="Vertical">
                        <Grid>
                            <Button x:Name="btnSelectedObject" Visibility="Hidden" Margin="0,3" Width="70" Height="70" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}" Click="btnSelectedObject_Click">
                                <Image Stretch="Uniform" Margin="7"/>
                            </Button>
                            <Button x:Name="btnSelectedColor" Content="" Foreground="White" FontSize="25" BorderBrush="LightGray" Background="Black" Style="{StaticResource ToolbarButton}" Click="btnSelectedColor_Click"/>
                            <Button x:Name="btnSelectedFloor" Visibility="Hidden" Margin="0,3" Width="70" Height="70" Background="Black" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}" Click="SelectFloorButtonClick">
                                <Grid>
                                    <Image x:Name="imgSelectedFloor" Stretch="Uniform" Margin="12"/>
                                    <TextBlock x:Name="txtWallWidth" VerticalAlignment="Center" HorizontalAlignment="Center" Foreground="White" FontSize="30" Text=""/>
                                </Grid>
                            </Button>
                        </Grid>
                        <Button Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.FREE}" Style="{StaticResource ToolbarButton}" Background="LightSkyBlue" BorderBrush="DeepSkyBlue">
                            <Image Stretch="UniformToFill" Margin="9" Source="/CampaignMaster;component/Resources/Images/Icons/ink_free.png"/>
                        </Button>
                        <Button Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.SNAP}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="UniformToFill" Margin="5" Source="/CampaignMaster;component/Resources/Images/Icons/ink_snap.png"/>
                        </Button>
                        <Button Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.SCRATCH}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="UniformToFill" Margin="5" Source="/CampaignMaster;component/Resources/Images/Icons/ink_scratch.png"/>
                        </Button>
                        <!--<Button Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.LINE}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="UniformToFill" Margin="8" Source="/CampaignMaster;component/Resources/Images/Icons/ink_line.png"/>
                        </Button>-->
                        <Button Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.SHAPE}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="UniformToFill" Margin="6,5,4,5" Source="/CampaignMaster;component/Resources/Images/Icons/ink_shape.png"/>
                        </Button>
                        <!--<Button Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.POLYGON}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="UniformToFill" Margin="5,4,5,6" Source="/CampaignMaster;component/Resources/Images/Icons/ink_polygon.png"/>
                        </Button>-->
                        <Button Margin="5,3" Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.ROOM}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="Fill" Margin="7" Source="/CampaignMaster;component/Resources/Images/Icons/icon_drawRoom.png"/>
                        </Button>
                        <Button Margin="5,3" Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.OBJECT_ADD}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="Fill" Margin="7" Source="/CampaignMaster;component/Resources/Images/Icons/icon_object.png"/>
                        </Button>
                        <Button Margin="5,3" Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.OBJECT_SNAP}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="Fill" Margin="7" Source="/CampaignMaster;component/Resources/Images/Icons/icon_object_snap.png"/>
                        </Button>
                    </StackPanel>

                    <StackPanel x:Name="pnlColorPalette" Grid.Column="1" Margin="0,5,5,5" Orientation="Vertical">
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="LightGray" Background="Black"/>
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="WhiteSmoke" Background="#FF323232"/>
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="#FF323232" Background="White"/>
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="LightSkyBlue" Background="#C2E5EF"/>
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="Moccasin" Background="Peru"/>
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="PapayaWhip" Background="#3d2814"/>
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="PaleGreen" Background="SeaGreen"/>
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="LightPink" Background="#B24747"/>
                    </StackPanel>
                    <!--<StackPanel x:Name="pnlFloorPalette" Visibility="Hidden" Grid.Column="1" Margin="0,5,5,5" Orientation="Vertical">
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="Gray" Background="#FF323232"/>
                        <Button Style="{StaticResource ColorButton}" Tag="False" BorderBrush="PapayaWhip" Background="#3d2814"/>
                        <ItemsControl Background="{x:Null}" ItemsSource="{Binding PaletteFloors}">
                            <ItemsControl.ItemTemplate>
                                <DataTemplate>
                                    <Button Margin="5,5,0,0" Width="70" Height="70" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}" Click="PaletteFloorButtonClick">
                                        <Image Stretch="Uniform" Margin="12" Source="{Binding ImageSource}"/>
                                    </Button>
                                </DataTemplate>
                            </ItemsControl.ItemTemplate>
                            <ItemsControl.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel IsItemsHost="True" Orientation="Vertical"/>
                                </ItemsPanelTemplate>
                            </ItemsControl.ItemsPanel>
                        </ItemsControl>
                    </StackPanel>-->
                    <ItemsControl x:Name="pnlObjectPalette" Grid.Column="1" Visibility="Hidden" Margin="0,5,5,5" Background="{x:Null}" ItemsSource="{Binding PaletteObjects}">
                        <ItemsControl.ItemTemplate>
                            <DataTemplate>
                                <Button Margin="5,5,0,0" Width="70" Height="70" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}" Click="PaletteObjectButtonClick">
                                    <Image Stretch="Uniform" Margin="7" Source="{Binding ImageSource}"/>
                                </Button>
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                        <ItemsControl.ItemsPanel>
                            <ItemsPanelTemplate>
                                <StackPanel IsItemsHost="True" Orientation="Vertical"/>
                            </ItemsPanelTemplate>
                        </ItemsControl.ItemsPanel>
                    </ItemsControl>
                </Grid>
            </Border>

            <!--<Border Grid.Row="2" VerticalAlignment="Bottom"  Background="{StaticResource Toolbar.Background}" CornerRadius="0,0,0,0" Margin="0,5,0,0">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <StackPanel x:Name="pnlToolBar2" Grid.Column="0" Margin="5,5,0,5" Orientation="Vertical">
                        <Button Margin="5,3" Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.OBJECT_ADD}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="Fill" Margin="7" Source="/CampaignMaster;component/Resources/Images/Icons/icon_object.png"/>
                        </Button>
                        -->
            <!--<Button Margin="5,3" Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.OBJECT_SNAP}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="Fill" Margin="7" Source="/CampaignMaster;component/Resources/Images/Icons/icon_define_object.png"/>
                        </Button>-->
            <!--
                        <Button Margin="5,3" Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.ROOM}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="Fill" Margin="7" Source="/CampaignMaster;component/Resources/Images/Icons/icon_drawRoom.png"/>
                        </Button>
                        <Button Click="DrawModeButton_Click" Tag="{x:Static misc:DrawMode.PROP_SELECT}" Style="{StaticResource ToolbarButton}">
                            <Image Stretch="Fill" Margin="12" Source="/CampaignMaster;component/Resources/Images/Icons/icon_furniture.png"/>
                        </Button>
                    </StackPanel>
                    <StackPanel Grid.Column="1" VerticalAlignment="Center" Margin="0,5,5,5" Orientation="Vertical">
                        -->
            <!--<Button Visibility="Hidden" Margin="5,0,5,3" Width="40" Height="40" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}" Click="btnSelectedObject_Click">
                            <Image Stretch="Uniform" Margin="7"/>
                        </Button>-->
            <!--<Button x:Name="btnSelectedObject" Margin="5,0,5,3" Width="70" Height="70" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}" Click="btnSelectedObject_Click">
                            <Image Stretch="Uniform" Margin="7"/>
                        </Button>
                        <Button x:Name="btnSelectedFloor" Margin="5,5,5,3" Width="70" Height="70" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}" Click="SelectFloorButtonClick">
                            <Image Stretch="Uniform" Margin="12"/>
                        </Button>-->
            <!--<Button x:Name="btnSelectedProp" Margin="5,3,5,3" Width="70" Height="70" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}">
                            <Image Stretch="Uniform" Margin="7"/>
                        </Button>-->
            <!--
                    </StackPanel>
                </Grid>
            </Border>-->

            <Border Grid.Row="3" Height="86" Background="{StaticResource Toolbar.Background}" CornerRadius="0,0,0,40" Margin="0,10,0,0">
                <TextBlock x:Name="txtSize" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,12" Foreground="WhiteSmoke" FontSize="65" Text="5"/>
            </Border>
        </Grid>
        <Border Grid.Column="1" Margin="0,-227" Background="{StaticResource Toolbar.Background}" CornerRadius="0,0,0,40">
            <Grid x:Name="grdAdditonalPalettes">
                <StackPanel x:Name="pnlAdditionalColorPalette" Margin="0,0,5,0" Orientation="Vertical">
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="LightGray" Background="#FF323232"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="Gray" Background="#FFC8C8C8"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="Red" Background="DarkRed"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="DarkRed" Background="Red"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="Gold" Background="Orange"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="DarkKhaki" Background="#FFF3FF00"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="Green" Background="GreenYellow"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="GreenYellow" Background="LimeGreen"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="GreenYellow" Background="ForestGreen"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="LightSkyBlue" Background="LightSeaGreen"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="#FF0097FF" Background="LightSkyBlue"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="LightSkyBlue" Background="#FF0097FF"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="LightSkyBlue" Background="#FF00558F"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="MediumSlateBlue" Background="DarkSlateBlue"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="Moccasin" Background="SaddleBrown"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="Moccasin" Background="Tan"/>
                    <Button Style="{StaticResource ColorButton}" Tag="False" Height="69" Width="69" BorderBrush="SaddleBrown" Background="Moccasin"/>
                </StackPanel>
                <ItemsControl x:Name="pnlAdditionalObjectPalette" Visibility="Hidden" Margin="0,5,5,5" Background="{x:Null}" ItemsSource="{Binding AdditionalPaletteObjects}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <Button Margin="5,5,0,0" Width="69" Height="69" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}" Click="PaletteObjectButtonClick">
                                <Image Stretch="Uniform" Margin="7" Source="{Binding ImageSource}"/>
                            </Button>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel IsItemsHost="True" Orientation="Vertical"/>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
                <ItemsControl x:Name="pnlFloorPalette" Visibility="Hidden" Background="{x:Null}" ItemsSource="{Binding PaletteFloors}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <Button Margin="5,5,0,0" Width="69" Height="69" BorderBrush="Gray" BorderThickness="3" Style="{StaticResource RoundButton}" Click="PaletteFloorButtonClick">
                                <Image Stretch="Uniform" Margin="12" Source="{Binding ImageSource}"/>
                            </Button>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <StackPanel IsItemsHost="True" Orientation="Vertical"/>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </Grid>
        </Border>
        <Grid Grid.Column="2" Margin="0,-227" Background="{StaticResource Toolbar.Background}">
            <ItemsControl x:Name="pnlCustomColors" Background="{x:Null}" ItemsSource="{Binding AdditionalPaletteColors}">
                <ItemsControl.ItemTemplate>
                    <DataTemplate>
                        <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="LightGray" Background="{Binding}"/>
                    </DataTemplate>
                </ItemsControl.ItemTemplate>
                <ItemsControl.ItemsPanel>
                    <ItemsPanelTemplate>
                        <WrapPanel IsItemsHost="True" Orientation="Vertical"/>
                    </ItemsPanelTemplate>
                </ItemsControl.ItemsPanel>
            </ItemsControl>
            <!--<WrapPanel Orientation="Vertical">
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="LightGray" Background="Black"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="LightGray" Background="#FF323232"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="LightGray" Background="#FF646464"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="LightGray" Background="#FF969696"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="Gray" Background="#FFC8C8C8"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="#FF323232" Background="White"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="LightPink" Background="Red"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="Gold" Background="Orange"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="DarkKhaki" Background="#FFF3FF00"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="Green" Background="GreenYellow"/>
