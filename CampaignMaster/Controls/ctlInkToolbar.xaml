                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="LightSkyBlue" Background="LightSeaGreen"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="#FF0097FF" Background="LightSkyBlue"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="LightSkyBlue" Background="#FF0097FF"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="LightSkyBlue" Background="#FF00558F"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="MediumSlateBlue" Background="DarkSlateBlue"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="Moccasin" Background="SaddleBrown"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="Moccasin" Background="Peru"/>
                <Button Style="{StaticResource ColorButton}" Height="69" Width="69" BorderBrush="Moccasin" Background="Tan"/>
