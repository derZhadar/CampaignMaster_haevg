            </WrapPanel>-->
            <Grid x:Name="pnlCustomObjects" Visibility="Hidden">
                <ItemsControl Background="{x:Null}" ItemsSource="{Binding CustomPaletteObjects}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <Button Margin="5,5,0,0" Width="69" Height="69" BorderBrush="Gray" BorderThickness="2" Style="{StaticResource RoundButton}" Click="ObjectButton_Clicked">
                                <Image Stretch="Uniform" Margin="7" Source="{Binding ImageSource}"/>
                            </Button>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <WrapPanel IsItemsHost="True" Orientation="Vertical"/>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </Grid>
            <Grid x:Name="grdFloors" Visibility="Hidden">
                <ItemsControl Background="{x:Null}" ItemsSource="{Binding Floors}">
                    <ItemsControl.ItemTemplate>
                        <DataTemplate>
                            <Button Margin="5,5,0,0" Width="70" Height="70" BorderBrush="Gray" BorderThickness="2" Style="{StaticResource RoundButton}" Click="FloorButtonClick">
                                <Image Stretch="Uniform" Margin="12" Source="{Binding}"/>
                            </Button>
                        </DataTemplate>
                    </ItemsControl.ItemTemplate>
                    <ItemsControl.ItemsPanel>
                        <ItemsPanelTemplate>
                            <WrapPanel IsItemsHost="True" Orientation="Vertical"/>
                        </ItemsPanelTemplate>
                    </ItemsControl.ItemsPanel>
                </ItemsControl>
            </Grid>
        </Grid>
    </Grid>
</UserControl>
